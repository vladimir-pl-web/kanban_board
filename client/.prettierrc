{
	"trailingComma": "none",
	"tabWidth": 1,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^/components/(.*)$",
		"^/layout/(.*)$",
		"^/ui/(.*)$",
		"^/providers/(.*)$",
		"^/constants/(.*)$",
		"^/types/(.*)$",
		"^/assets/(.*)$",
		"^/config/(.*)$",
		"^/store/(.*)$",
		"^/books/(.*)$",
		"^/hooks/(.*)$",
		"^/utils/(.*)$",
		"^/api/(.*)$",
		"^../(.*)$",
		"^./(.*)$",
		"^(.scss)$"
	],
	"importOrderSeparation": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
